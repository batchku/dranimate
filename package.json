  "description": "Dranimate web app",
  "scripts": {
    "build": "npm run clean && npm run build:webpack && npm run build:copy",
    "build:copy": "npm run build:copy:arap; cp src/manifest.json dist/manifest.json",
    "build:copy:arap": "mkdir dist/arap; cp src/arap/deform2d.js dist/arap/deform2d.js",
    "build:webpack": "webpack --colors --progress --display-error-details",
