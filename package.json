{
  "name": "Dranimate",
  "version": "0.0.1",
  "description": "Dranimate web app",
  "scripts": {
    "build": "npm run clean && npm run build:webpack && npm run build:copy",
    "build:copy": "cp src/assets/favicon.ico dist/favicon.ico; cp src/manifest.json dist/manifest.json; cp src/assets/dranimate.png dist/dranimate.png; cp -r src/assets dist/assets;",
    "build:webpack": "webpack --colors --progress --display-error-details",
    "clean": "rm -rf dist",
    "dev": "webpack-dev-server --inline --colors --progress --content-base .",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.32",
    "@babel/preset-env": "7.0.0-beta.32",
    "@babel/preset-react": "7.0.0-beta.32",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0-beta.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.6.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "delaunay-fast": "^1.0.1",
    "file-saver": "^1.3.3",
    "leapjs": "^0.6.4",
    "leapjs-plugins": "^0.1.12",
    "prop-types": "^15.6.0",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-router-dom": "^4.2.2",
    "three": "^0.88.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmuartfab/dranimate-browser.git"
  },
  "bugs": {
    "url": "https://github.com/cmuartfab/dranimate-browser/issues"
  },
  "homepage": "https://github.com/cmuartfab/dranimate-browser#readme",
  "license": "ISC",
  "main": "index.js",
  "author": ""
}
