    "angular-messages": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "delaunay-fast": "^1.0.1",
    "file-saver": "^1.3.3",
    "gif.js": "^0.2.0",
    "jquery": "^3.1.1",
    "three": "^0.88.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.31",
    "@babel/preset-env": "^7.0.0-beta.31",
    "babel-loader": "^8.0.0-beta.0",
    "script-loader": "^0.7.2",
