  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-cookies": "^1.5.8",
    "angular-material": "^1.1.1",
    "angular-messages": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "script-loader": "^0.7.0",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  }
}
