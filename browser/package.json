{
  "name": "browser",
  "version": "1.0.0",
  "description": "Dranimate browser application",
  "main": "index.js",
  "scripts": {
    "build:copy": "cp ./index.html dist/index.html; cp ./main.css dist/main.css; cp -r ./lib_remove_me dist/lib_remove_me",
